<div class="container px-5 px-sm-0">
  <div class="row">

    <div class="col-lg-8 ">
<h5 class="text-left">新規会員登録</h5>


<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "customers/shared/error_messages", resource: resource %>

 <div class="form-group row">
    <div class="col-sm-4"><%= f.label :名前 %></div>
    <div class="col-sm-4">(姓)<%= f.text_field :last_name, autofocus: true,placeholder:"令和" %></div>
   <div class="col-sm-4">(名)<%= f.text_field :first_name, autofocus: true ,placeholder:"道子"%></div>
  </div>

  <div class="form-group row">
     <div class="col-sm-4"><%= f.label :フリガナ %></div>
     <div class="col-sm-4">(セイ)<%= f.text_field :last_name_kana, autofocus: true,placeholder:"レイワ" %></div>
     <div class="col-sm-4">(メイ)<%= f.text_field :first_name_kana, autofocus: true,placeholder:"ミチコ" %></div>
  </div>

  <div class="form-group row">
     <div class="col-sm-4"><%= f.label :メールアドレス %></div>
     <div class="col-sm-6"><%= f.email_field :email, autofocus: true, autocomplete: "email",placeholder:"sample@example.com" %></div>
  </div>

 <div class="form-group row">
     <div class="col-sm-4"><%= f.label :郵便番号 %></div>
     <div class="col-sm-4"><%= f.text_field :postal_code, autofocus: true,placeholder:"0000000" %></div>
  </div>

  <div class="form-group row">
     <div class="col-sm-4"><%= f.label :住所 %></div>
     <div class="col-sm-4"><%= f.text_field :address, autofocus: true,placeholder:"東京都渋谷区代々木神園待ち0-0" %></div>
  </div>

 <div class="form-group row">
     <div class="col-sm-4"><%= f.label :電話番号 %></div>
     <div class="col-sm-4"><%= f.text_field :phone_number, autofocus: true,placeholder:"0000000000" %></div>
  </div>

  <div class="form-group row">
     <div class="col-sm-4"><%= f.label :パスワード %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em></div>
    <% end %><br>
     <div class="col-sm-4"><%= f.password_field :password, autocomplete: "new-password" %></div>
  </div>

  <div class="form-group row">
     <div class="col-sm-4"><%= f.label :パスワード %></div>
     <div class="col-sm-4"><%= f.password_field :password_confirmation, autocomplete: "new-password" %></div>
  </div>

  <div class>
    <%= f.submit "新規登録",class:"btn btn-primary px-3" %>
  </div>
<% end %>

<h5>既に登録済みの方</h5>
<p><%= link_to "こちら",customers_sign_in_path %>からログインしてください。</p>

</div>
</div>
</diV>

